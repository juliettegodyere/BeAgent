<!DOCTYPE html>
<html>
<head>
	<title>Search</title>
    <link rel="stylesheet" type="text/css" href="/stylesheets/style.css">
    <link href="css/bootstrap.min.css" rel="stylesheet">
    <link href="css/responsive.css" rel="stylesheet">
    <link href="css/font-awesome.min.css" rel="stylesheet">
</head>
<body>
    <header>
        <% include ./header %>

    </header>
            <h2 style="color: #000; text-align: center;margin-top: 50px"><b>Matching Items</b></h2>


<div id="photos">
	
 	
        <% photos.forEach(function(photo){ %>
            <div class="photo">
                <div>
                  <img src='<%=photo.path%>'>  
                </div>
                 <div style="text-align: left">
                    <p class=""><%=photo.name%></p>
                </div>
                 <div style="text-align: left">
                     <p class=""><%=photo.location%></p>
                </div>
                 <div style="text-align: left">
                    <p class="">&#8358 <%=photo.amount%></p>
                </div>
                 <div style="text-align: left">
                    <p class=""> <%=photo.datePosted%></p>
                </div>
                
                
                <div>
                     <button type="submit" class="btn btn-primary pull-right btn-sm">Order</button>
                
                </div>
               
                
               
            </div>
        <% })%>
        </div> 
        <footer>
                    <% include ./footer %>

        </footer>
     <script src="js/jquery.js" type="text/javascript"></script>
    <script src="js/bootstrap.min.js" type="text/javascript"></script>

</body>
</html>